{
  "name": "graphql-default",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen:default": "graphql-codegen --require dotenv/config --config codegen-default.ts dotenv_config_path=node_modules/dev-envs/.dev.hasura.env",
    "codegen:role": "graphql-codegen --require dotenv/config --config codegen-role.ts dotenv_config_path=node_modules/dev-envs/.dev.hasura.env",
    "codegen:role+naming": "graphql-codegen --require dotenv/config --config codegen-role-naming.ts dotenv_config_path=node_modules/dev-envs/.dev.hasura.env",
    "codegen:role+naming+enum": "graphql-codegen --require dotenv/config --config codegen-role-naming-enum.ts dotenv_config_path=node_modules/dev-envs/.dev.hasura.env"
  },
  "dependencies": {
    "@urql/core": "^3.1.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "next": "^13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ui": "workspace:*",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/plugin-helpers": "^3.1.2",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "dev-envs": "workspace:*",
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.4"
  }
}
